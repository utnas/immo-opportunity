<div class="alert alert-info">
<h3>  Building {{model.name}} details </h3>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Multiplier of gross revenu</th>
          <th>Price per appartment</th>
          <th>Net value of rental</th>
          <th>Operating expenses</th>
          <th>Current performance</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><span class="label label-danger">{{model.multiplierOfGrossRevenu}}</span></td>
          <td><span class="label label-warning">{{model.pricePerFlat}}</span></td>
          <td><span class="label label-danger">{{model.netValuesOfRents}}</span></td>
          <td><span class="label label-danger">{{model.operatingExpenses}}</span></td>
          <td><span class="label label-danger">{{model.currentPerformance}} %</span></td>
        </tr>
      </tbody>
    </table>

  </div>
</div>
